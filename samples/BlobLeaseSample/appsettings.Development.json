{
  "BlobLeasing": {
    "StorageAccountUri": "https://[YOUR_STORAGE_ACCOUNT].blob.core.windows.net",
    "ContainerName": "leases-dev",
    "CreateContainerIfNotExists": true,
    "KeyPrefix": "dev-sample-",
    "DefaultLeaseDuration": "00:00:20",
    "AutoRenew": true,
    "AutoRenewInterval": "00:00:13",
    "AutoRenewRetryInterval": "00:00:03",
    "AutoRenewMaxRetries": 3,
    "AutoRenewSafetyThreshold": 0.9,
    "Authentication": {
      "Mode": "DefaultAzureCredential"
    },
    "ConnectionString": null
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Information",
      "Microsoft.Hosting.Lifetime": "Information",
      "DistributedLeasing": "Debug",
      "System": "Information"
    }
  }
}
